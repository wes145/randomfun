#include <iostream>

using namespace std;

int main() {
    // Input variables
    int N, A, B;
    cin >> N >> A >> B;

    // FizzBuzz logic
    for (int i = 1; i <= N; ++i) {
        // Check for both A and B first to handle FizzBuzz case
        if (i % A == 0 && i % B == 0) {
            cout << "FizzBuzz" << endl;
        } else if (i % A == 0) {
            cout << "Fizz" << endl;
        } else if (i % B == 0) {
            cout << "Buzz" << endl;
        } else {
            cout << i << endl;
        }
    }

    return 0;
}
